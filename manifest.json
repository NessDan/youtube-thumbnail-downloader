{
  "manifest_version": 3,
  "name": "NessDan's YouTube Thumbnail Downloader",
  "version": "1.0",
  "description": "Download YouTube video thumbnails",
  "permissions": ["activeTab", "downloads"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
